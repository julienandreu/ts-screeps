{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "prettier",
    "prettier/@typescript-eslint"
  ],
  "plugins": [
    "import",
    "jest",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "rules": {
    "import/no-default-export": "off"
  }
}
